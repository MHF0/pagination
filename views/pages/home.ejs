<%-include('../partial/header')-%>
<div class=" container">
        <center>
        <div style="width:auto; border-style: solid;border-color: black;">
        <table class="table table-border table-hover">
            <thead class="bg-warning">
                <tr>
                    <th>
                        First Name
                    </th>
                    <th>
                       Last Name
                    </th>
                </tr>
            </thead>
            <tbody>
                <% employees.forEach(function(emp) { %>
                <tr>
                    <td>
                        <%= emp.firstname %>
                    </td>
                    <td>
                        <%= emp.lastname %>
                    </td>
                </tr>
                <% }); %>
            </tbody>
        </table>
        <nav aria-label="Page navigation example">
            <ul class="pagination">
              <li class="page-item"><a class="page-link" href="#">Previous</a></li>
              <li class="page-item"><a class="page-link" href="/?page=1&size=3">1</a></li>
              <li class="page-item"><a class="page-link" href="/?page=2&size=3">2</a></li>
              <li class="page-item"><a class="page-link" href="/?page=3&size=3">3</a></li>
              <li class="page-item"><a class="page-link" href="#">Next</a></li>
            </ul>
          </nav>
    </div>
    </center>         

<%-include('../partial/footer')-%>